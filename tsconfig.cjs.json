{
  "compilerOptions": {
    "rootDir": "src/",
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.cjs.tsbuildinfo",
    "declaration": true,
    // "emitDeclarationOnly": true,
    "skipLibCheck": true,
    "target": "ESNext",
    "module": "CommonJS",
    "moduleResolution": "Node10",
    "outDir": "dist/cjs",
    "noImplicitAny": true
  },
  "include": ["src/*.ts"],
  "exclude": ["__tests__/"]
}
